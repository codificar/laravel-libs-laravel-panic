{
    "name": "codificar/laravel-panic",
    "description": "This lib provides the backend feature to allow the users and providers to send a distress call to the admin and the registered emergency contacts",
    "type": "package",
    "authors": [
        {
            "name": "Codificar",
            "email": "libs@codificar.com.br"
        }
    ],
    "prefer-stable": true,
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Codificar\\Panic\\": "src/",
            "Tests\\": "tests/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Codificar\\Panic\\": "src/"
        }
    },
    "require": {
        "guzzlehttp/guzzle": "~6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    }
}